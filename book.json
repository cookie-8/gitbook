{
    "title": "cookie8 的文档库",
    "author": "cookie8",
    "description": "cookie8 的文档库,里面是前端文章",
    "language": "zh-hans",
    "styles": {
      "website": "./extra/css/gitbook-configure.css"
    },
    "plugins": [
      "theme-comscore",
      "prism",
      "-highlight",
      "copy-code-button",
      "search-pro",
      "-search",
      "-lunr",
      "expandable-chapters",
      "splitter",
      "-sharing",
      "github-buttons",
      "tbfed-pagefooter",
      "anchor-navigation-ex"
    ],
    "pluginsConfig": {
      "github-buttons": {
        "buttons": [
          {
            "user": "cookie8",
            "repo": "", 
            "type": "star",
            "count": true,
            "size": "small"
          }, 
          {
            "user": "cookie8",
            "width": "160", 
            "type": "follow", 
            "count": true,
            "size": "small"
          }
        ]
      },
      "prism": {
        "css": [
          "prismjs/themes/prism-solarizedlight.css"
        ],
        "lang": {
          "shell": "bash"
        }
      },
      "tbfed-pagefooter": {
        "copyright":"copyright reserved",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
      },
      "anchor-navigation-ex": {
        "showLevel": false
      }
    }
  }
  